{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Object",
  "description": "An object schema",
  "properties": {
    "array": {
      "type": "array",
      "title": "Array (Items is an object)",
      "description": "An array schema",
      "items": {
        "type": "boolean",
        "title": "Boolean (Any Of)",
        "description": "A boolean schema",
        "anyOf": [
          { "const": true, "title": "True", "description": "The value \"true\"" },
          { "const": false, "title": "False", "description": "The value \"false\"" }
        ]
      }
    }
  }
}
