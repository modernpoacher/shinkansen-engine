{
  "name": "shinkansen-engine",
  "version": "1.5.0",
  "description": "Shinkansen Engine",
  "keywords": [
    "Shinkansen",
    "Engine",
    "Zashiki Karakuri",
    "Zashiki",
    "Karakuri",
    "React",
    "React Router"
  ],
  "main": "./src/index.mjs",
  "type": "module",
  "types": "./index.d.mts",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "https://modernpoacher.com"
  },
  "license": "ISC",
  "engines": {
    "node": ">=22.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/modernpoacher/shinkansen-engine.git"
  },
  "scripts": {
    "build": "cross-env DEBUG=shinkansen-engine* webpack",
    "build-storybook": "storybook build",
    "build:watch": "cross-env DEBUG=shinkansen-engine* webpack --watch",
    "lint": "eslint",
    "lint:fix": "npm run lint -- --fix",
    "nvm": "bash nvm.sh",
    "prelint": "npm run nvm",
    "prepare": "husky",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prestorybook": "npm run build && npm run transform",
    "pretest": "npm run nvm",
    "storybook": "storybook dev -p 6010",
    "test": "npm run test:mocha && npm run test:jest",
    "test:jest": "cross-env NODE_ENV=test jest",
    "test:mocha": "cross-env NODE_ENV=test mocha test --recursive",
    "transform": "cross-env DEBUG=shinkansen-engine* ./transform.sh"
  },
  "dependencies": {
    "debug": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-syntax-import-attributes": "^7.26.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/register": "^7.25.9",
    "@modernpoacher/design-system": "1.1.31",
    "@sequencemedia/eslint-config-standard": "^0.2.41",
    "@sequencemedia/eslint-config-typescript": "^0.1.57",
    "@storybook/addon-actions": "^8.6.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.5",
    "@storybook/manager-api": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-webpack5": "^8.6.4",
    "@storybook/theming": "^8.6.4",
    "@types/babel__register": "^7.17.3",
    "@types/chai": "^5.2.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/react": "18.3.13",
    "@types/react-test-renderer": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "autoprefixer": "^10.4.20",
    "chai": "^5.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.41.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-storybook": "^0.11.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "mocha": "^11.1.0",
    "perfectionist": "^2.4.0",
    "postcss": "^8.5.3",
    "postcss-discard-comments": "^7.0.3",
    "postcss-discard-empty": "^7.0.0",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-map": "^0.11.0",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "^7.3.0",
    "react-test-renderer": "18.3.1",
    "remove-files-webpack-plugin": "^1.5.0",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "shinkansen-cogs": "1.9.0",
    "shinkansen-gears": "1.3.0",
    "shinkansen-pantograph": "1.5.0",
    "shinkansen-pinion": "1.4.0",
    "shinkansen-rails": "1.1.0",
    "shinkansen-signals": "1.2.0",
    "shinkansen-sprockets": "1.3.0",
    "shinkansen-transmission": "2.5.0",
    "sinon": "^19.0.2",
    "storybook": "^8.6.4",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "shinkansen-cogs": "1.9.x",
    "shinkansen-gears": "1.3.x",
    "shinkansen-pantograph": "1.5.x",
    "shinkansen-pinion": "1.4.x",
    "shinkansen-rails": "1.1.x",
    "shinkansen-signals": "1.2.x",
    "shinkansen-sprockets": "1.3.x",
    "shinkansen-transmission": "2.5.x"
  },
  "imports": {
    "#build/paths": "./build/paths.mjs",
    "#engine": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "#engine/components": {
      "require": "./src/components/index.cjs",
      "import": "./src/components/index.mjs"
    },
    "#engine/components/*": {
      "require": "./src/components/*/index.cjs",
      "import": "./src/components/*/index.mjs"
    },
    "#engine/components/gears/forward": "./src/components/gears/forward/index.cjs",
    "#engine/components/gears/reverse": "./src/components/gears/reverse/index.cjs",
    "#engine/engine": "./src/engine/index.cjs",
    "#engine/transformers": "./src/transformers/index.mjs",
    "#engine/transformers/*": "./src/transformers/*/index.mjs",
    "#stories/components/*": "./stories/components/*/index.stories.jsx",
    "#stories/definitions/*": "./stories/definitions/*.json",
    "#stories/engine/*": "./stories/engine/*/index.stories.jsx",
    "#stories/errors": "./stories/errors.mjs",
    "#stories/state": "./stories/state/index.jsx",
    "#stories/state/*": "./stories/state/*/index.jsx"
  },
  "exports": {
    ".": {
      "require": "./src/index.cjs",
      "import": "./src/index.mjs"
    },
    "./components": {
      "require": "./src/components/index.cjs",
      "import": "./src/components/index.mjs"
    },
    "./components/*": {
      "require": "./src/components/*/index.cjs",
      "import": "./src/components/*/index.mjs"
    },
    "./components/gears/forward": "./src/components/gears/forward/index.cjs",
    "./components/gears/reverse": "./src/components/gears/reverse/index.cjs",
    "./engine": "./src/engine/index.cjs",
    "./transformers": "./src/transformers/index.mjs",
    "./transformers/*": "./src/transformers/*/index.mjs"
  }
}
